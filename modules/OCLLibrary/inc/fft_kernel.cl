/*
 only support one block , and base with 2
*/
__kernel void fft_1D_R2C_kernel( __global double   *in,         
						         __global double2  *out,   
							     const  int N, 
							     const  int num,					           
							     __local  double2  *lMem)  
{
	int tId_Bx  = get_local_id(0);
	int tId_Gx = get_global_id(0);

	int i, j, k, le,lei, lej, thea;
	int num2 = num>>1;
	double val1, val2;
	double2  t; 
	float2  c;

	lMem[tId_Bx].x = in[tId_Gx];
	lMem[tId_Bx].y = 0; 
	barrier(CLK_LOCAL_MEM_FENCE);

	j = tId_Bx;
	k = 0;
	for(i=0; i<N; i++)
	{
		k <<= 1;
		k += j&0x00000001;
		j >>= 1; 
	}

	if(k>tId_Bx)
	{
		val1 = lMem[tId_Bx].x;
		lMem[tId_Bx].x = lMem[k].x;	
		lMem[k].x = val1;
	}
	barrier(CLK_LOCAL_MEM_FENCE);
	
	for(i=0; i<N; i++)
	{
		le  = 2<<i;
		lei = le>>1;
		lej = tId_Bx%le;
		thea = lej*1024/lei;
		c = cossin_t[thea];

		if ( lej < lei )
		{
			lej = tId_Bx + lei;
			t.x = c.x * lMem[lej].x - c.y * lMem[lej].y;
			t.y = c.x * lMem[lej].y + c.y * lMem[lej].x;
			lMem[lej] = lMem[tId_Bx] - t; 
			lMem[tId_Bx] += t;
		}
		barrier(CLK_LOCAL_MEM_FENCE);
	}
	
	out[tId_Gx] = lMem[tId_Bx]/num;
}

