		c = cossin_t[thea];
		c.y *= dir;

		if ( lej < lei )
		{
			lej = tId_Bx + lei;
			t.x = c.x * lMem[lej].x - c.y * lMem[lej].y;
			t.y = c.x * lMem[lej].y + c.y * lMem[lej].x;
			lMem[lej] = lMem[tId_Bx] - t; 
			lMem[tId_Bx] += t;
		}
		barrier(CLK_LOCAL_MEM_FENCE);
	}
	//transpose
	barrier(CLK_GLOBAL_MEM_FENCE);
	if(1 == dir)
		out[get_group_id(0) + tId_Bx*num_y] = lMem[tId_Bx]/num_x;
	else 
		out[get_group_id(0) + tId_Bx*num_y] = lMem[tId_Bx];
}